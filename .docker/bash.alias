alias cls="clear"
alias spotteleop="ros2 run teleop_twist_keyboard teleop_twist_keyboard"
alias spotviz="rviz2 -d /opt/ros/lcas/spot_ros2/spot_description/share/spot_description/rviz/viz_spot.rviz"
alias spotsit="ros2 service call /sit std_srvs/srv/Trigger"
alias spotstand="ros2 service call /sit std_srvs/srv/Trigger"
alias spotrollover="ros2 service call /rollover std_srvs/srv/Trigger"
alias spoton="ros2 service call /power_on std_srvs/srv/Trigger"
alias spotoff="ros2 service call /power_off std_srvs/srv/Trigger"
alias spotstop="ros2 service call /stop std_srvs/srv/Trigger"